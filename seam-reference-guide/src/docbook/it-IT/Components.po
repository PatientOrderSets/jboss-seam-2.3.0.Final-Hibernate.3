#, no-c-format
msgid "org.jboss.seam.core.conversationList"
msgstr "org.jboss.seam.core.conversationList"

#. Tag: para
#: Components.xml:674
#, no-c-format
msgid "Manager component for the conversation list."
msgstr "Componente gestore per la lista delle conversazioni."

