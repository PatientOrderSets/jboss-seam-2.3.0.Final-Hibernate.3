#: Controls.xml:22
#, no-c-format
msgid ""
"<![CDATA[<html xmlns=\"http://www.w3.org/1999/xhtml\"\n"
"      xmlns:s=\"http://jboss.org/schema/seam/taglib\">]]>"
msgstr ""
"<![CDATA[<html xmlns=\"http://www.w3.org/1999/xhtml\"\n"
"      xmlns:s=\"http://jboss.org/schema/seam/taglib\">]]>"

