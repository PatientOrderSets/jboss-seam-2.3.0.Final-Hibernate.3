        </component>
    </tag>

    <tag>
        <tag-name>series</tag-name>
        <component>
            <component-type>org.jboss.seam.pdf.ui.UIChartSeries</component-type>
        </component>
    </tag>

    <tag>
        <tag-name>color</tag-name>
        <component>
            <component-type>org.jboss.seam.pdf.ui.UIColor</component-type>
        </component>
    </tag>

    <tag>
        <tag-name>stroke</tag-name>
        <component>
            <component-type>org.jboss.seam.pdf.ui.UIStroke</component-type>
        </component>
    </tag>

    <tag>
        <tag-name>html</tag-name>
        <component>
            <component-type>org.jboss.seam.pdf.ui.UIHtmlText</component-type>
        </component>
    </tag>

    <tag>
    	<tag-name>swing</tag-name>
    	<component>
    		<component-type>org.jboss.seam.pdf.ui.UISwingComponent</component-type>
    	</component>
